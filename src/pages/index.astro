      frontmatter={{ title: "前端导航", tableOfContents: true }}
      hasSidebar={false}
      headings={[
        { depth: 2, text: "AI工具", slug: "ai工具" },
        { depth: 2, text: "日常", slug: "日常" },
        { depth: 2, text: "论坛与团队", slug: "论坛与团队" },
